<script runat=server>
Platform.Load("Core", "1.1.5");
var api = new Script.Util.WSProxy();


var req = api.retrieve("DataFolder", ["Name","ParentFolder.ID"], { 
        Property: "ID", 
        SimpleOperator: "equals", 
        Value: 23329 
    });

    var name = req.Results[0].Name;
    var id = req.Results[0].ParentFolder.ID;

    Write("<br>name: " + name)
    Write("<br>id: " + id)

</script>
