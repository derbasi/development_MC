<script runat=server>
Platform.Load("Core", "1.1.5");
var api = new Script.Util.WSProxy();

var folderName = "MyNewFolder5000"

// Create new Folder

var parentFolderId = 23234; // Your first parent folder
var props = {
    "Name": folderName,
    "Description": "API Created Folder",
    "ParentFolder": {
        ID : parentFolderId,
        IDSpecified: true
    },
    "IsActive": true,
    "IsEditable": true,
    "AllowChildren": true,
    "ContentType": "dataextension"
};
// execute folder creation
var result = api.createItem("DataFolder", props); 
  




var req = api.retrieve("DataFolder", ["ID"], {
    Property: "Name",
    SimpleOperator: "equals",
    Value: folderName
});

var newId = req.Results[0].NewID;
Write("newId: " + newId);


</script>
