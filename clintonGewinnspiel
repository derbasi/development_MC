<script language="javascript" runat="server">
  Platform.Load("core", "1.1");
    try {
    var prox = new Script.Util.WSProxy();
    var result = prox.retrieve(
      "QueryDefinition", 
      ["ObjectID"],                      // Status, DataExtensionTarget.Name, ObjectID, CustomerKey, CreatedDate, TargetUpdateType 
      {
        Property: "Name",                   // Select search parameters with Name, ObjectID, CustomerKey
        SimpleOperator: "equals",
        Value: "getGewinnspielName"
      }
    );
    Write(Stringify(result));
  } catch(e) {
    Write(Stringify(e));
  }
</script>